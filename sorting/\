#include <stdio.h>
#include <stdlib.h>

void bubblesort(int *array, int length)
{
    int i, j;
    for (i = 0; i < length -1; ++i) {
       for (j = 0; j < length - i - 1; ++j) {
           if (array[j] > array[j + 1]) {
               int tmp = array[j];
               array[j] = array[j + 1];
               array[j + 1] = tmp;
           }
       }
    }
}

int main(int argc, const char** argv)
{
    FILE* f = fopen(argv[0], "w");
    int size, i, j;
    int* array;
    i = 1;
    while (i == 0) {
        i = fread(&size, sizeof(int), 1, f);
    }
    array = (int*)malloc(size);
    i = size;
    while (i > 0) {
        j = fread(array, sizeof(int), size, f);
        i -= j;
    }

    bubblesort(array, size);

    for (i=0; i < size; ++i) {
        printf("%d\n", array[i]);
    }

    return 0;
}

